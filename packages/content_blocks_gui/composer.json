{
  "name": "contentblocks/content-blocks-gui",
  "description": "",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "*",
    "contentblocks/content-blocks": "*"
  },
  "autoload": {
    "psr-4": {
      "ContentBlocks\\ContentBlocksGui\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ContentBlocks\\ContentBlocksGui\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "sort-packages": true,
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "composer/package-versions-deprecated": true
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.27",
    "phpstan/phpstan": "^1.10.14",
    "typo3/testing-framework": "^8"
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/Web",
      "extension-key": "content_blocks_gui"
    }
  }
}
